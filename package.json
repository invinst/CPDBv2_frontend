  "scripts": {
    "build": "CPDB_APP_ENV=prod webpack",
    "build-staging": "CPDB_APP_ENV=staging webpack",
    "build-beta": "CPDB_APP_ENV=beta webpack",
    "start": "CPDB_APP_ENV=dev webpack-dev-server",
    "test": "CPDB_APP_ENV=test webpack-dev-server",
    "test-grep": "yarn test --pm-grep",
    "test-watch": "yarn test --pm-watch",
    "cover": "CPDB_APP_ENV=test nyc webpack-dev-server",
    "cover-report": "nyc report --reporter=text-lcov | coveralls",
    "live-test": "CPDB_APP_ENV=live-test wdio wdio.conf.js",
    "lint": "eslint ./src ./test ./selenium-test",
    "lint-fix": "eslint ./src ./test ./selenium-test --fix",
    "docker-build": "sed -e \"s/{{MOBILE_SERVER_NAME}}/$MOBILE_SERVER_NAME/\" nginx.conf.template > nginx.conf && docker build -t cpdbdev/desktop-frontend:latest . && rm nginx.conf",
    "docker-run": "docker run --rm -d --name desktop-frontend -p $FRONTEND_PORT:80 cpdbdev/desktop-frontend:latest",
    "docker-kill": "docker kill desktop-frontend",
    "test-nginx": "mocha test_nginx.js",
    "local-test-nginx": "export MOBILE_SERVER_NAME=m.cpdp.co && export FRONTEND_PORT=80 && export FRONTEND_DOMAIN=localhost && yarn docker-build && yarn docker-run && yarn test-nginx ; yarn docker-kill"
  },
  "repository": {
    "type": "git",
    "url": ""
